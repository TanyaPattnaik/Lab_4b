#include <iostream>
using namespace std;
int factorial(int n)
{ 
        if(n==1)
        {
              return 1;
        }
        else
        {
              return n*factorial(n-1);
        }
}

int main()
{
        int num;
        cout<<"\nENTER NUMBER: ";
        cin>>num;
        cout<<"\nFactorial of the number is: "<<factorial(num);
        return 0;
}
